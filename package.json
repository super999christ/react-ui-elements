{
  "name": "@pickleballinc/react-ui",
  "version": "1.3.6",
  "description": "React UI lib for @pickleballinc npm projects.",
  "main": "./src/index.ts",
  "scripts": {
    "build:rollup": "rimraf dist && rollup -c",
    "build": "rm -rf build && rm -rf tsconfig.build.tsbuildinfo && npm run build:legacy && npm run build:modern && npm run build:node && npm run build:stable && npm run build:types && npm run build:copy-files && npm run build:css",
    "build:legacy": "cross-env NODE_ENV=production node ./scripts/build.mjs legacy",
    "build:modern": "cross-env NODE_ENV=production node ./scripts/build.mjs modern",
    "build:node": "cross-env NODE_ENV=production node ./scripts/build.mjs node",
    "build:stable": "cross-env NODE_ENV=production node ./scripts/build.mjs stable",
    "build:copy-files": "cross-env NODE_ENV=production node ./scripts/copyFiles.mjs",
    "build:types": "cross-env NODE_ENV=production node ./scripts/buildTypes.mjs",
    "build:css": "cross-env NODE_ENV=production gulp css",
    "dev": "rollup -c -w",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "lint": "eslint .",
    "format": "eslint --fix && prettier '**/*.{json,yaml}' --write --ignore-path .gitignore",
    "check-types": "tsc --noEmit --pretty && tsc --project cypress --noEmit --pretty"
  },
  "peerDependencies": {
    "@floating-ui/react": "^0.25.0",
